---
syntax: bash
tags: [bash, personal]
version: 1.0.0
default: NO
---

# Prints version information
append-to-top --version;
# Runs script in verbose mode
append-to-top -v;
# Runs script in quiet mode
append-to-top -q;
# pipes output into a file
append-to-top -o file.txt;

# append from file
append-to-top file-to-append.txt file-to-get-new-text.txt

# append to file from text
append-to-top file-to-append.txt "testing"
